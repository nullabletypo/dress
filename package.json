  "dependencies": {
    "@emotion/hash": "^0.8.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.5",
    "@types/node": "^14.0.24",
    "@types/react": "^16.9.43",
    "@types/react-dom": "^16.9.8",
    "@typoerr/eslint-config": "^0.0.34",
    "eslint": "^7.5.0",
    "jest": "^26.1.0",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.0.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "rimraf": "^3.0.2",
