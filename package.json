{
  "name": "dress",
  "description": "css-in-js",
  "version": "0.0.4",
  "author": "nullabletypo",
  "files": [
    "dist",
    "README.md",
    "LICENCE"
  ],
  "main": "dist/cjs/index.js",
  "jsnext:main": "dist/esm/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/cjs/index.d.ts",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nullabletypo/dress.git"
  },
  "bugs": {
    "url": "https://github.com/nullabletypo/dress/issues"
  },
  "homepage": "https://github.com/nullabletypo/dress",
  "dependencies": {
    "@emotion/hash": "^0.7.1"
  },
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "@types/node": "^11.13.2",
    "@types/react": "^16.8.13",
    "@types/react-dom": "^16.8.3",
    "eslint": "^5.16.0",
    "eslint-config-nullabletypo": "https://github.com/nullabletypo/eslint-config-nullabletypo",
    "gzip-size-cli": "^3.0.0",
    "jest": "^24.7.1",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.3",
    "prettier": "^1.16.4",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "rimraf": "^2.6.3",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.0.3",
    "tsconfig-paths": "^3.8.0",
    "typescript": "^3.4.2"
  },
  "keywords": [],
  "scripts": {
    "example": "parcel example/index.html --port 3000 --out-dir example/dist --cache-dir example/.cache --open",
    "compile": "npm-run-all compile:**",
    "compile:cjs": "tsc --module commonjs --outDir dist/cjs",
    "compile:esm": "tsc --module es2015 --outDir dist/esm",
    "clean": "run-p clean:**",
    "clean:dist": "rimraf dist",
    "lint": "eslint src/**/*.ts",
    "release:github": "git add . && git commit -am $npm_package_version --allow-empty && git tag $npm_package_version && git push && git push --tags",
    "release": "run-s test clean compile  release:github",
    "size": "parcel build src/index.ts --no-cache --out-dir size && node -e \"process.stdout.write('gzip size: ')\" && gzip-size size/index.js && rimraf size",
    "test": "NODE_ENV=test jest"
  }
}
