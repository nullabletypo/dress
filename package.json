  "keywords": [],
  "scripts": {
    "start": "NODE_ENV=development webpack-dev-server --mode=development --config=\"examples/webpack.config.js\"",
    "compile": "npm-run-all compile:**",
    "compile:cjs": "tsc --module commonjs --outDir dist/cjs",
    "compile:esm": "tsc --module es2015 --outDir dist/esm",
    "clean": "run-p clean:**",
    "clean:dist": "rimraf dist",
    "lint": "tslint src/**/*.ts",
    "release:github": "git add . && git commit -am $npm_package_version --allow-empty && git tag $npm_package_version && git push && git push --tags",
    "release": "run-s clean compile test release:github",
    "size": "webpack --mode=production --config=webpack.size.config.js && node -e \"process.stdout.write('gzip size: ')\" && gzip-size pref/bundle-size.js && rimraf pref",
    "test": "NODE_ENV=test jest"
  }
}
