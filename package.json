  },
  "keywords": [],
  "scripts": {
    "example": "parcel example/index.html --port 3000 --out-dir example/dist --cache-dir example/.cache --open",
    "compile": "run-p compile:*",
    "compile:esm": "tsc --outDir dist/esm",
    "compile:cjs": "tsc --outDir dist/cjs --module commonjs",
    "clean": "run-p clean:**",
    "clean:dist": "rimraf dist",
    "lint": "eslint src/**/*.ts",
    "release:github": "git add . && git commit -am $npm_package_version --allow-empty && git tag $npm_package_version && git push && git push --tags",
    "release": "run-s test clean bundle release:github",
    "test": "NODE_ENV=test jest"
  },
  "jest": {
    "preset": "ts-jest",
    "verbose": true,
    "globals": {
      "ts-jest": {
        "tsConfig": "test/tsconfig.json"
      }
    }
  }
