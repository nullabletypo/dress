  "devDependencies": {
    "@types/jest": "^24.0.11",
    "@types/react": "^16.8.10",
    "@types/react-dom": "^16.8.3",
    "eslint": "^5.15.3",
    "eslint-config-nullabletypo": "https://github.com/nullabletypo/eslint-config-nullabletypo",
    "gzip-size-cli": "^3.0.0",
    "jest": "^24.5.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.16.4",
