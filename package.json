  "devDependencies": {
    "@types/jest": "^24.0.12",
    "@types/node": "^12.0.0",
    "@types/react": "^16.8.16",
    "@types/react-dom": "^16.8.4",
    "eslint": "^5.16.0",
    "eslint-config-nullabletypo": "https://github.com/nullabletypo/eslint-config-nullabletypo",
    "gzip-size-cli": "^3.0.0",
    "jest": "^24.7.1",
    "np": "^5.0.1",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.3",
    "prettier": "^1.17.0",
